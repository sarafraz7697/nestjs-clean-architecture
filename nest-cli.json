{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nest-solid/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nest-solid/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nest-solid",
  "projects": {
    "audit-logger": {
      "type": "application",
      "root": "apps/audit-logger",
      "entryFile": "main",
      "sourceRoot": "apps/audit-logger/src",
      "compilerOptions": {
        "tsConfigPath": "apps/audit-logger/tsconfig.app.json"
      }
    },
    "auth-module": {
      "type": "application",
      "root": "apps/auth-module",
      "entryFile": "main",
      "sourceRoot": "apps/auth-module/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-module/tsconfig.app.json"
      }
    },
    "gateway-module": {
      "type": "application",
      "root": "apps/gateway-module",
      "entryFile": "main",
      "sourceRoot": "apps/gateway-module/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway-module/tsconfig.app.json"
      }
    },
    "nest-solid": {
      "type": "application",
      "root": "apps/nest-solid",
      "entryFile": "main",
      "sourceRoot": "apps/nest-solid/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-solid/tsconfig.app.json"
      }
    }
  }
}